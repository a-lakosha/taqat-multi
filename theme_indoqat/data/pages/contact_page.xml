<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <template name="Indoqat Contact Us" id="indoqat_contact_us_page" inherit_id="website.contactus"
                  active="True">
            <xpath expr="//div[@id='wrap']" position="replace">
                <div id="wrap" class="oe_structure oe_empty">
                    <section class="pt80 pb80">
                        <div class="container py-lg-5 o_animate o_anim_fade_in">
                            <div class="map_container o_not_editable">
                                <div class="css_non_editable_mode_hidden">
                                    <div class="missing_option_warning alert alert-info rounded-0 fade show d-none d-print-none o_default_snippet_text">
                                        An address must be specified for a map to be embedded
                                    </div>
                                </div>
                                <div class="top-section d-flex flex-column align-items-center gap-3">
                                    <h1 class="display-5 text-primary" style="font-weight:700;">Contact us</h1>

                                    <h3 class="h1 mb-lg-5 text-center">
                                        We’re always excited to hear from you!
                                    </h3>
                                </div>


                                <div class="map d-flex flex-column position-relative">
                                    <div class="floating-details bg-white d-flex flex-column p-3 gap-lg-4">
                                        <div class="address-bulding d-flex flex-column gap-2 ">
                                            <h3 class="display-5">
                                                <svg width="24" height="28" viewBox="0 0 24 28" fill="none"
                                                     xmlns="http://www.w3.org/2000/svg"
                                                     class="me-2"
                                                >
                                                    <path d="M19.5425 20.2091C18.2937 21.458 15.6821 24.0695 13.8851 25.8665C12.8437 26.9079 11.1569 26.9085 10.1155 25.8671C8.34983 24.1014 5.78883 21.5404 4.45757 20.2091C0.291976 16.0435 0.291976 9.28979 4.45757 5.12419C8.62316 0.958602 15.3769 0.958602 19.5425 5.12419C23.7081 9.28979 23.7081 16.0435 19.5425 20.2091Z"
                                                          stroke="#343434" stroke-width="2.66667"
                                                          stroke-linecap="round"
                                                          stroke-linejoin="round"/>
                                                    <path d="M16 12.6667C16 14.8758 14.2092 16.6667 12 16.6667C9.7909 16.6667 8.00004 14.8758 8.00004 12.6667C8.00004 10.4575 9.7909 8.66667 12 8.66667C14.2092 8.66667 16 10.4575 16 12.6667Z"
                                                          stroke="#343434" stroke-width="2.66667"
                                                          stroke-linecap="round"
                                                          stroke-linejoin="round"/>
                                                </svg>

                                                Main building
                                            </h3>
                                            <p class="subaddress">
                                                PT. IndoQat World Wide™
                                                GRIYA UPAKARA, Jl. Cikini IV No.10, CIKINI, MENTENG, JAKARTA PUSAT, DKI
                                                JAKARTA 10330
                                            </p>
                                        </div>
                                        <div class="email-address  d-flex flex-column gap-2 ">
                                            <h3 class="email display-5 ">
                                                <svg width="28" height="22" viewBox="0 0 28 22" fill="none"
                                                     xmlns="http://www.w3.org/2000/svg"
                                                     class="me-2"
                                                >
                                                    <path d="M2 5.66699L12.5208 12.6809C13.4165 13.278 14.5835 13.278 15.4792 12.6809L26 5.66699M4.66667 20.3337H23.3333C24.8061 20.3337 26 19.1398 26 17.667V4.33366C26 2.8609 24.8061 1.66699 23.3333 1.66699H4.66667C3.19391 1.66699 2 2.8609 2 4.33366V17.667C2 19.1398 3.19391 20.3337 4.66667 20.3337Z"
                                                          stroke="#343434" stroke-width="2.66667"
                                                          stroke-linecap="round"
                                                          stroke-linejoin="round"/>
                                                </svg>

                                                E-mail

                                            </h3>
                                            <a href="mailto:info@indoqat.com" class="mb-3 text-dark">
                                                info@indoqat.com
                                            </a>
                                        </div>
                                        <div class="phone-number  d-flex flex-column gap-2 ">
                                            <h3 class="number display-5">
                                                <svg width="28" height="28" viewBox="0 0 28 28" fill="none"
                                                     xmlns="http://www.w3.org/2000/svg"
                                                     class="me-2"
                                                >
                                                    <path d="M2 4.66667C2 3.19391 3.19391 2 4.66667 2H9.03899C9.61289 2 10.1224 2.36724 10.3039 2.9117L12.301 8.90295C12.5108 9.53243 12.2258 10.2204 11.6324 10.5172L8.62268 12.022C10.0923 15.2816 12.7184 17.9077 15.978 19.3773L17.4828 16.3676C17.7796 15.7742 18.4676 15.4892 19.0971 15.699L25.0883 17.6961C25.6328 17.8776 26 18.3871 26 18.961V23.3333C26 24.8061 24.8061 26 23.3333 26H22C10.9543 26 2 17.0457 2 6V4.66667Z"
                                                          stroke="#343434" stroke-width="2.66667"
                                                          stroke-linecap="round"
                                                          stroke-linejoin="round"/>
                                                </svg>

                                                phone number
                                            </h3>
                                            <div>


                                                <a class="mb-1 text-dark" href="tel:+07433620133">
                                                    +074-33620133
                                                </a>
                                                <p class="mb-1">
                                                    Sunday - Thursday: 7:30 AM - 3:30 PM
                                                </p>
                                                <p class="m-0">
                                                    Friday/Saturday: Closed (Public Holiday)
                                                </p>
                                            </div>
                                        </div>

                                    </div>


                                    <iframe class="s_map_embedded o_not_editable" width="100%"
                                            frameborder="0"
                                            scrolling="no" marginheight="0" marginwidth="0" aria-label="Map"
                                            src="https://maps.google.com/maps?q=RR4P%2BWW2%20Cikini%2C%20Central%20Jakarta%20City%2C%20Jakarta%2C%20Indonesia&amp;t=m&amp;z=12&amp;ie=UTF8&amp;iwloc=&amp;output=embed"/>
                                </div>
                                <div class="s_map_color_filter"/>
                            </div>
                        </div>
                    </section>


                    <section class="s_website_form mb-5 mb-lg-0 pb80 pt80  contact-bottom-section" data-vcss="001"
                             data-snippet="s_website_form"
                             data-name="Form">
                        <div class="container py-lg-5 ">
                            <div class="d-flex flex-column flex-lg-row gap-4 ">


                                <div class="d-flex flex-column  align-items-center align-items-lg-start text-lg-start text-center gap-4 o_animate o_anim_slide_in o_anim_from_left">
                                    <h2>
                                        Contact Us
                                    </h2>
                                    <p class="display-5">
                                        Have a question? Interested in collaborating with us? Let’s connect with you
                                        ASAP!

                                    </p>

                                </div>

                                <form id="contactus_form" action="/website/form/" method="post"
                                      enctype="multipart/form-data"
                                      class="o_mark_required bg-o-color-2-light  p-4 o_animate o_anim_slide_in  o_anim_from_right"
                                      data-mark=""
                                      data-pre-fill="true" data-model_name="crm.lead"
                                      data-success-mode="message"
                                      data-success-page="/contactus-thank-you">
                                    <input type="hidden" name="csrf_token" t-att-value="request.csrf_token()"/>
                                    <input type="hidden" name="team_id" t-att-value="1"/>
                                    <div class="s_website_form_rows row s_col_no_bgcolor">
                                        <div data-name="Field"
                                             class="s_website_form_field mb-3 col-12 s_website_form_model_required"
                                             data-type="char">
                                            <label class="s_website_form_label" style="width: 200px"
                                                   for="o1lbmbg7zofi">
                                                <span class="s_website_form_label_content">Full name</span>
                                                <span class="s_website_form_mark"/>
                                            </label>
                                            <input class="form-control s_website_form_input" type="text"
                                                   name="name"
                                                   required="" placeholder="Full name" id="o1lbmbg7zofi"
                                                   data-fill-with="name"/>
                                        </div>
                                        <div data-name="Field"
                                             class="s_website_form_field mb-3 col-12 s_website_form_required"
                                             data-type="tel">
                                            <label class="s_website_form_label" style="width: 200px"
                                                   for="oivu4lehvc1r">
                                                <span class="s_website_form_label_content">Phone Number
                                                </span>
                                                <span class="s_website_form_mark"/>
                                            </label>
                                            <input class="form-control s_website_form_input" type="tel"
                                                   name="phone"
                                                   required="1" placeholder="Phone Number" id="oivu4lehvc1r"
                                                   data-fill-with="phone"/>
                                        </div>
                                        <div data-name="Field"
                                             class="s_website_form_field mb-3 col-12 s_website_form_required"
                                             data-type="email">
                                            <label class="s_website_form_label" style="width: 200px"
                                                   for="ovoq76ldksre">
                                                <span class="s_website_form_label_content">Your Email</span>
                                                <span class="s_website_form_mark"/>
                                            </label>
                                            <input class="form-control s_website_form_input" type="email"
                                                   name="email_from" required="1" placeholder="Your Email"
                                                   id="ovoq76ldksre" data-fill-with="email"/>
                                        </div>
                                        <div data-name="Field"
                                             class="s_website_form_field mb-3 col-12 s_website_form_required"
                                             data-type="text">
                                            <label class="s_website_form_label" style="width: 200px"
                                                   for="oca7dw0b530s">
                                                <span class="s_website_form_label_content">Your Message
                                                </span>
                                                <span class="s_website_form_mark"/>
                                            </label>
                                            <textarea class="form-control s_website_form_input"
                                                      name="description"
                                                      required="1" placeholder="Your Message" id="oca7dw0b530s"
                                                      rows="5"/>
                                        </div>
                                        <div
                                                class="mb-0 col-12 s_website_form_submit d-flex align-items-center flex-column flex-lg-row-reverse justify-content-lg-between"
                                                data-name="Submit Button">
                                            <a href="#" role="button"
                                               class="btn btn-primary w-100 w-lg-auto d-flex align-items-center justify-content-between flex-nowrap s_website_form_send o_default_snippet_text">
                                                Send
                                                <i class="bi bi-arrow-right ms-2"/>
                                            </a>
                                            <span id="s_website_form_result" class=""/>
                                        </div>
                                    </div>
                                </form>
                                <div class="s_website_form_end_message h-100 bg-light py-5 d-none">
                                    <div class="oe_structure">
                                        <section class="s_text_block" data-snippet="s_text_block">
                                            <div class="container">
                                                <h2 class="text-center">
                                                    <span class="fa fa-check-circle text-primary"/>
                                                    Thank You For Your
                                                    Message
                                                </h2>
                                                <p class="text-center">Our team will message you back as
                                                    soon as possible.
                                                </p>
                                            </div>
                                        </section>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </section>
                </div>
            </xpath>
        </template>
    </data>
</odoo>