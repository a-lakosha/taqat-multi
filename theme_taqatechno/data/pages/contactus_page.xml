<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <template
                name="Contact Us Template"
                id="taqatechno_contactus_page"
                inherit_id="website.contactus"
                active="True"
        >
            <xpath expr="//div[@id='wrap']" position="replace">
                <div role="main" id="wrap" class="oe_structure oe_empty">
                    <div class="oe_structure oe_empty"/>
                    <section class="contact-us py-lg-5">
                        <div class="container py-5">
                            <div class="row">
                                <div class="content pb-5 mb-lg-5 text-center">
                                    <h1 class="fs-6 text-primary pb-2">Contact Us</h1>
                                    <h2 style="max-width: 656px" class="pb-3 m-auto">
                                        Ready to take the next step in your digital journey?
                                    </h2>
                                    <h6 style="max-width: 656px" class="m-auto">
                                        Ready to take the next step in your digital journey? Contact
                                        us for consultations, assistance, or insights, and let's
                                        achieve great results together.
                                    </h6>
                                </div>

                                <!-- Contact Info Cards -->
                                <div class="col-lg-4 pb-3">
                                    <div
                                            style="background: #5a2163; padding: 40px 32px"
                                            class="box rounded-5 h-100 text-center p-5"
                                    >
                                        <svg
                                                width="40"
                                                height="40"
                                                viewBox="0 0 40 40"
                                                fill="none"
                                                xmlns="http://www.w3.org/2000/svg"
                                                aria-hidden="true"
                                        >
                                            <path
                                                    d="M36.25 11.25V28.75C36.25 30.8211 34.5711 32.5 32.5 32.5H7.5C5.42893 32.5 3.75 30.8211 3.75 28.75V11.25M36.25 11.25C36.25 9.17893 34.5711 7.5 32.5 7.5H7.5C5.42893 7.5 3.75 9.17893 3.75 11.25M36.25 11.25V11.6545C36.25 12.9567 35.5744 14.1657 34.4654 14.8482L21.9654 22.5405C20.7601 23.2823 19.2399 23.2823 18.0346 22.5405L5.53463 14.8482C4.42558 14.1657 3.75 12.9567 3.75 11.6545V11.25"
                                                    stroke="#F9F5F9"
                                                    stroke-width="2.5"
                                                    stroke-linecap="round"
                                                    stroke-linejoin="round"
                                            />
                                        </svg>
                                        <h6 class="text-white pt-4">Email Us</h6>
                                        <p class="text-white pt-2">
                                            <t t-esc="website.company_id.email"/>
                                        </p>
                                    </div>
                                </div>

                                <div class="col-lg-4 pb-3">
                                    <div
                                            style="background: #5a2163; padding: 40px 32px"
                                            class="box rounded-5 h-100 text-center p-5"
                                    >
                                        <svg
                                                width="36"
                                                height="36"
                                                viewBox="0 0 36 36"
                                                fill="none"
                                                xmlns="http://www.w3.org/2000/svg"
                                                aria-hidden="true"
                                        >
                                            <path
                                                    d="M1.75 9.25C1.75 23.0571 12.9429 34.25 26.75 34.25H30.5C32.5711 34.25 34.25 32.5711 34.25 30.5V28.214C34.25 27.3536 33.6644 26.6036 32.8298 26.3949L25.4578 24.5519C24.7259 24.369 23.9557 24.6424 23.503 25.246L21.886 27.402C21.4167 28.0277 20.605 28.3044 19.8706 28.0354C14.359 26.0164 9.98359 21.641 7.96461 16.1294C7.69556 15.395 7.97227 14.5833 8.598 14.114L10.754 12.497C11.3576 12.0443 11.631 11.2741 11.4481 10.5422L9.60506 3.17025C9.39639 2.33556 8.64642 1.75 7.78604 1.75H5.5C3.42893 1.75 1.75 3.42893 1.75 5.5V9.25Z"
                                                    stroke="#F5F4F7"
                                                    stroke-width="2.5"
                                                    stroke-linecap="round"
                                                    stroke-linejoin="round"
                                            />
                                        </svg>
                                        <h6 class="text-white pt-4">Call Us</h6>
                                        <p class="text-white pt-2">
                                            <t t-esc="website.company_id.phone"/>
                                        </p>
                                    </div>
                                </div>

                                <div class="col-lg-4 pb-3">
                                    <div
                                            style="background: #5a2163; padding: 40px 32px"
                                            class="box rounded-5 h-100 text-center"
                                    >
                                        <svg
                                                width="40"
                                                height="40"
                                                viewBox="0 0 40 40"
                                                fill="none"
                                                xmlns="http://www.w3.org/2000/svg"
                                                aria-hidden="true"
                                        >
                                            <path
                                                    d="M25 17.5C25 20.2614 22.7614 22.5 20 22.5C17.2386 22.5 15 20.2614 15 17.5C15 14.7386 17.2386 12.5 20 12.5C22.7614 12.5 25 14.7386 25 17.5Z"
                                                    stroke="#F5F4F7"
                                                    stroke-width="2.5"
                                                    stroke-linecap="round"
                                                    stroke-linejoin="round"
                                            />
                                            <path
                                                    d="M32.5 17.5C32.5 29.4036 20 36.25 20 36.25C20 36.25 7.5 29.4036 7.5 17.5C7.5 10.5964 13.0964 5 20 5C26.9036 5 32.5 10.5964 32.5 17.5Z"
                                                    stroke="#F5F4F7"
                                                    stroke-width="2.5"
                                                    stroke-linecap="round"
                                                    stroke-linejoin="round"
                                            />
                                        </svg>
                                        <h6 class="text-white pt-4">Visit Us</h6>
                                        <p class="text-white pt-2">
                                            <t t-esc="website.company_id.street"/>
                                        </p>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </section>
                    <div class="oe_structure oe_empty"/>
                    <!-- Message Form -->
                    <section class="contact-us py-5 mt-lg-5" style="background: #f5f4f7">
                        <div class="container">
                            <div class="row">
                                <div class="col-lg-6">
                                    <div class="content">
                                        <h2 class="h3 pb-lg-3">
                                            Send us a message
                                        </h2>
                                        <p class="fs-6">
                                            Fill out this form and we'll get back to you ASAP.
                                        </p>
                                    </div>
                                </div>
                                <div class="col-lg-6">
                                    <section
                                            class="s_website_form pt16 pb16 o_colored_level"
                                            data-vcss="001"
                                            data-snippet="s_website_form"
                                            data-name="Form"
                                            style="background-image: none"
                                    >
                                        <div class="container">
                                            <form
                                                    action="/website/form/"
                                                    method="post"
                                                    enctype="multipart/form-data"
                                                    class="o_mark_required p-4 rounded-5 bg-white"
                                                    data-mark="*"
                                                    data-pre-fill="true"
                                                    data-model_name="crm.lead"
                                                    data-success-mode="message"
                                                    data-success-page="/contactus-thank-you"
                                            >
                                                <div class="s_website_form_rows row s_col_no_bgcolor">
                                                    <!-- First / Last Name -->
                                                    <div class="row pb-2">
                                                        <div class="col-lg-6">
                                                            <label for="o21d3azj6qlq" class="pb-3">First name</label>
                                                            <div
                                                                    data-name="Field"
                                                                    class="s_website_form_field mb-3 col-12 s_website_form_required"
                                                                    data-type="char"
                                                                    data-translated-name="Field"
                                                            >
                                                                <div class="row s_col_no_resize s_col_no_bgcolor">
                                                                    <div class="col-sm">
                                                                        <input
                                                                                style="padding: 16px 20px"
                                                                                class="form-control s_website_form_input rounded-5"
                                                                                type="text"
                                                                                name="contact_name"
                                                                                required="1"
                                                                                placeholder="Name"
                                                                                id="o21d3azj6qlq"
                                                                                data-fill-with="name"
                                                                                aria-label="First name"
                                                                        />
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="col-lg-6">
                                                            <label for="o3gzq5mazd7l" class="pb-3">Last name</label>
                                                            <div
                                                                    data-name="Field"
                                                                    class="s_website_form_field mb-3 col-12 s_website_form_custom"
                                                                    data-type="char"
                                                                    data-translated-name="Field"
                                                            >
                                                                <div class="row s_col_no_resize s_col_no_bgcolor">
                                                                    <div class="col-sm">
                                                                        <input
                                                                                style="padding: 16px 20px"
                                                                                class="form-control s_website_form_input rounded-5"
                                                                                type="text"
                                                                                name="last_name"
                                                                                placeholder="Last name"
                                                                                id="o3gzq5mazd7l"
                                                                                data-fill-with="undefined"
                                                                                aria-label="Last name"
                                                                        />
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>

                                                    <!-- Email -->
                                                    <label for="ovt0gdenlqkf" class="pb-3">Email</label>
                                                    <div
                                                            data-name="Field"
                                                            class="s_website_form_field pb-2 mb-3 col-12 s_website_form_required"
                                                            data-type="email"
                                                            data-translated-name="Field"
                                                    >
                                                        <div class="row s_col_no_resize s_col_no_bgcolor">
                                                            <div class="col-sm">
                                                                <input
                                                                        style="padding: 16px 20px"
                                                                        class="form-control s_website_form_input rounded-5"
                                                                        type="email"
                                                                        name="email_from"
                                                                        required="1"
                                                                        placeholder="email@email.com"
                                                                        data-fill-with="email"
                                                                        id="ovt0gdenlqkf"
                                                                        aria-label="Email"
                                                                />
                                                            </div>
                                                        </div>
                                                    </div>

                                                    <!-- Phone -->
                                                    <div class="row pb-2">
                                                        <div class="col-12">
                                                            <label for="obkhe6w6lt4p" class="pb-3">Phone</label>
                                                            <div
                                                                    data-name="Field"
                                                                    class="s_website_form_field mb-3 col-12"
                                                                    data-type="tel"
                                                                    data-translated-name="Field"
                                                            >
                                                                <div class="row s_col_no_resize s_col_no_bgcolor">
                                                                    <div class="col-sm">
                                                                        <input
                                                                                style="padding: 16px 20px"
                                                                                class="form-control s_website_form_input rounded-5"
                                                                                type="tel"
                                                                                name="phone"
                                                                                placeholder="Phone"
                                                                                data-fill-with="phone"
                                                                                id="obkhe6w6lt4p"
                                                                                aria-label="Phone"
                                                                        />
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>

                                                    <!-- Subject -->
                                                    <label for="o1bpts8g68em" class="pb-3">Subject</label>
                                                    <div
                                                            data-name="Field"
                                                            class="s_website_form_field mb-3 col-12 pb-2 s_website_form_model_required"
                                                            data-type="char"
                                                            data-translated-name="Field"
                                                    >
                                                        <div class="row s_col_no_resize s_col_no_bgcolor">
                                                            <div class="col-sm">
                                                                <input
                                                                        style="padding: 16px 20px"
                                                                        class="form-control s_website_form_input rounded-5"
                                                                        type="text"
                                                                        name="name"
                                                                        required=""
                                                                        id="o1bpts8g68em"
                                                                        aria-label="Subject"
                                                                />
                                                            </div>
                                                        </div>
                                                    </div>

                                                    <!-- Message -->
                                                    <label for="o3tdzal1m2xj" class="pb-3">Message</label>
                                                    <div
                                                            data-name="Field"
                                                            class="s_website_form_field mb-3 col-12 s_website_form_required"
                                                            data-type="text"
                                                            data-translated-name="Field"
                                                    >
                                                        <div class="row s_col_no_resize s_col_no_bgcolor">
                                                            <div class="col-sm">
                                                                <textarea
                                                                        class="form-control s_website_form_input rounded-5"
                                                                        name="description"
                                                                        required="1"
                                                                        id="o3tdzal1m2xj"
                                                                        rows="6"
                                                                        aria-label="Message"
                                                                ></textarea>
                                                            </div>
                                                        </div>
                                                    </div>

                                                    <!-- Hidden team_id -->
                                                    <div
                                                            data-name="Field"
                                                            class="s_website_form_field mb-3 col-6 s_website_form_dnone"
                                                    >
                                                        <div class="row s_col_no_resize s_col_no_bgcolor">
                                                            <div>
                                                                <input
                                                                        style="padding: 16px 20px"
                                                                        type="hidden"
                                                                        class="form-control s_website_form_input"
                                                                        name="team_id"
                                                                        value="1"
                                                                />
                                                            </div>
                                                        </div>
                                                    </div>

                                                    <!-- Submit -->
                                                    <div
                                                            class="mb-0 py-2 pb-3 col-12 s_website_form_submit"
                                                            data-name="Submit Button"
                                                    >
                                                        <div class="s_website_form_label"/>
                                                        <button
                                                                type="submit"
                                                                role="button"
                                                                class="btn pe-3 align-items-center btn-primary s_website_form_send o_default_snippet_text"
                                                        >
                                                            <span class="d-inline-block pe-2">Send message</span>
                                                            <svg
                                                                    width="20"
                                                                    height="18"
                                                                    viewBox="0 0 20 18"
                                                                    fill="none"
                                                                    xmlns="http://www.w3.org/2000/svg"
                                                                    aria-hidden="true"
                                                            >
                                                                <path
                                                                        d="M11.5 1.5L19 9M19 9L11.5 16.5M19 9H1"
                                                                        stroke="#F9F5F9"
                                                                        stroke-width="1.5"
                                                                        stroke-linecap="round"
                                                                        stroke-linejoin="round"
                                                                />
                                                            </svg>
                                                        </button>
                                                        <span
                                                                class="pt-3 w-100 d-block"
                                                                id="s_website_form_result"
                                                        />
                                                    </div>
                                                </div>
                                            </form>

                                            <div class="s_website_form_end_message d-none">
                                                <div class="oe_structure">
                                                    <section
                                                            class="s_text_block pt64 pb64 o_colored_level o_cc"
                                                            data-snippet="s_text_block"
                                                    >
                                                        <div class="container">
                                                            <h2 class="text-center text-primary">
                                                                <span class="fa text-primary fa-check-circle"/>
                                                                Thank You For Your Feedback
                                                            </h2>
                                                            <p class="text-center text-primary">
                                                                Our team will message you back as soon as possible.
                                                                <br/>
                                                                In the meantime we invite you to visit our
                                                                <a href="/">website</a>.
                                                                <br/>
                                                            </p>
                                                        </div>
                                                    </section>
                                                </div>
                                            </div>
                                        </div>
                                    </section>
                                </div>
                            </div>
                        </div>
                    </section>
                    <div class="oe_structure oe_empty"/>
                    <!-- FAQ Section -->
                    <section class="py-5">
                        <div class="container my-lg-5 py-lg-5">
                            <div class="row">
                                <h2 class="text-lg-center pb-5 mb-lg-5 mx-auto"
                                    style="max-width: 534px"
                                >
                                    FAQâ€™s
                                </h2>
                                <t t-call="theme_taqatechno.collapse_custom"/>
                            </div>
                        </div>
                    </section>
                    <div class="oe_structure oe_empty"/>
                </div>
            </xpath>
        </template>
    </data>
</odoo>
