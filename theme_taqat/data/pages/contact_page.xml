<?xml version="1.0" encoding="UTF-8"?>
<odoo>
  <data noupdate="1">
    <template name="Taqat Contact Us" id="taqat_contact_us_page" inherit_id="website.contactus"
      active="True">
      <xpath expr="//div[@id='wrap']" position="replace">
        <t t-set="nav_dark" t-value="True" />
        <div id="wrap" class="oe_structure oe_empty">
          <section class="pt-5 bg-dark text-white contact-landing">
            <div class="container py-lg-5">
              <div class="d-flex flex-column align-items-center text-center py-5">
                <h1 class="h2 text-primary">Contact us</h1>
                <h2 class="display-2 fw-bold mb-4" style="max-width:690px;">Stay connected with us
                  and
                  start your
                  journey to
                  making an impact.
                </h2>
                <p class="display-4" style="max-width:880px;">We are here to connect, collaborate,
                  and
                  provide support. If you have any questions, feedback, or inquiries about our
                  programs and projects, we would love to hear from you. Reach out to us through the
                  available channels, and let's work together to create a positive impact.
                </p>
              </div>
              <div class="row align-content-stretch mb-5">
                <div class="col-lg-6">
                  <section class="s_map" data-map-type="m" data-map-zoom="12" data-snippet="s_map"
                    data-map-address="TAQAT Trading &amp; Business Solutions" data-name="Map">
                    <div class="map_container o_not_editable">
                      <div class="css_non_editable_mode_hidden">
                        <div
                          class="missing_option_warning alert alert-info rounded-0 fade show d-none d-print-none o_default_snippet_text">
                          An address must be specified for a map to be embedded
                        </div>
                      </div>
                      <iframe class="s_map_embedded o_not_editable" width="100%" height="100%"
                        frameborder="0" scrolling="no" marginheight="0" marginwidth="0"
                        aria-label="Map"
                        src="https://maps.google.com/maps?q=TAQAT%20Trading%20%26%20Business%20Solutions&amp;t=m&amp;z=12&amp;ie=UTF8&amp;iwloc=&amp;output=embed" />
                      <div class="s_map_color_filter" />
                    </div>
                  </section>
                </div>
                <div class="col-lg-6 order-lg-first">
                  <div class="py-5 p-lg-4">
                    <div class="mb-4 pb-3">
                      <h4 class="display-5 fw-bold">
                        <i class="bi bi-geo-alt me-1" /> Main building </h4>
                      <p class="m-0">8th Floor QFB Building, Building: 29 Suhaim Bin Hamad
                        Street,
                        Street: 901, Ring Road, Al Sadd - Area: 38 Doha - Qatar
                      </p>
                    </div>
                    <div class="mb-4 pb-3" t-if="website.company_id.email">
                      <h4 class="display-5 fw-bold">
                        <i class="bi bi-envelope me-1" /> Email </h4>
                      <a t-att-href="'mailto: %s' %website.company_id.email" class="d-block">
                        <span t-field="website.company_id.email" />
                      </a>
                    </div>
                    <div class="" t-if="website.company_id.mobile">
                      <h4 class="display-5 fw-bold">
                        <i class="bi bi-telephone me-1" /> Phone number </h4>
                      <a t-att-href="'tel: %s' %website.company_id.mobile" class="d-block">
                        <span t-field="website.company_id.mobile" />
                      </a>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </section>
          <section class="s_text_block py-5 my-lg-5 o_colored_level " data-snippet="s_text_block">
            <div class="container s_allow_columns">
              <div class="row">
                <div class="col-lg-6">
                  <h2 class="pb-3">Contact us</h2>
                  <p class="display-5">To contact the Center for Excellence in specific Humanitarian
                    and Development Work.
                  </p>
                </div>
                <div class="col-lg-6">
                  <section class="s_website_form" data-vcss="001" data-snippet="s_website_form"
                    data-name="Form">
                    <form id="contactus_form" action="/website/form/" method="post"
                      enctype="multipart/form-data" class="o_mark_required" data-mark=""
                      data-pre-fill="true" data-model_name="crm.lead" data-success-mode="message"
                      data-success-page="/contactus-thank-you">
                      <div class="s_website_form_rows row s_col_no_bgcolor">
                        <div data-name="Field"
                          class="s_website_form_field mb-3 col-12 s_website_form_model_required"
                          data-type="char">
                          <label class="s_website_form_label" style="width: 200px"
                            for="o1lbmbg7zofi">
                            <span class="s_website_form_label_content">Full name</span>
                            <span class="s_website_form_mark" />
                          </label>
                          <input class="form-control s_website_form_input" type="text" name="name"
                            required="" placeholder="Full name" id="o1lbmbg7zofi"
                            data-fill-with="name" />
                        </div>
                        <div data-name="Field"
                          class="s_website_form_field mb-3 col-12 s_website_form_required"
                          data-type="tel">
                          <label class="s_website_form_label" style="width: 200px"
                            for="oivu4lehvc1r">
                            <span class="s_website_form_label_content">Phone Number
                            </span>
                            <span class="s_website_form_mark" />
                          </label>
                          <input class="form-control s_website_form_input" type="tel" name="phone"
                            required="1" placeholder="Phone Number" id="oivu4lehvc1r"
                            data-fill-with="phone" />
                        </div>
                        <div data-name="Field"
                          class="s_website_form_field mb-3 col-12 s_website_form_required"
                          data-type="email">
                          <label class="s_website_form_label" style="width: 200px"
                            for="ovoq76ldksre">
                            <span class="s_website_form_label_content">Your Email</span>
                            <span class="s_website_form_mark" />
                          </label>
                          <input class="form-control s_website_form_input" type="email"
                            name="email_from" required="1" placeholder="Your Email"
                            id="ovoq76ldksre" data-fill-with="email" />
                        </div>
                        <div data-name="Field"
                          class="s_website_form_field mb-3 col-12 s_website_form_required"
                          data-type="text">
                          <label class="s_website_form_label" style="width: 200px"
                            for="oca7dw0b530s">
                            <span class="s_website_form_label_content">Your Message
                            </span>
                            <span class="s_website_form_mark" />
                          </label>
                          <textarea class="form-control s_website_form_input" name="description"
                            required="1" placeholder="Your Message" id="oca7dw0b530s" rows="3" />
                        </div>
                        <div
                          class="mb-0 col-12 s_website_form_submit d-flex align-items-center flex-column flex-lg-row-reverse justify-content-lg-between"
                          data-name="Submit Button">
                          <a href="#" role="button"
                            class="btn btn-primary w-100 w-lg-auto d-flex align-items-center justify-content-between flex-nowrap s_website_form_send o_default_snippet_text">
                            <span class="">Send</span>
                            <svg xmlns="http://www.w3.org/2000/svg" width="24" height="25"
                              viewBox="0 0 24 25" fill="none">
                              <path d="M15 19.8965L8 12.8965L15 5.89648" stroke="#FEFDF9"
                                stroke-width="2" stroke-linecap="round" stroke-linejoin="round" />
                            </svg>
                          </a>
                          <span id="s_website_form_result" class="" />
                        </div>
                      </div>
                    </form>
                    <div class="s_website_form_end_message h-100 bg-light py-5 d-none">
                      <div class="oe_structure">
                        <section class="s_text_block" data-snippet="s_text_block">
                          <div class="container">
                            <h2 class="text-center">
                              <span class="fa fa-check-circle text-primary" /> Thank You For Your
                              Message </h2>
                            <p class="text-center">Our team will message you back as
                              soon as possible.
                            </p>
                          </div>
                        </section>
                      </div>
                    </div>
                  </section>
                </div>
              </div>
            </div>
          </section>
        </div>
      </xpath>
    </template>
  </data>
</odoo>